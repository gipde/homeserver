extern "C" {
#include "../../main/ds18x20lib.h"
}
#include "Ds18x20libTest.h"

extern "C" {
	void event(uint8_t,uint32_t);
}

CASE(Ds18x20LibTest,TestBase)

uint32_t state;

void event(uint8_t type,uint32_t value) {
	// Hier muss eine CRC Funktion rein, die das Ergebnis berechnet
	state += type;
	if (value != UNDEF) {
		state += value;
	}
}

SETUP() {
	debug("Setting up");
	state=0;
}

TEST(test1) {
	debug("Hello from Test1");
	debug("state %d",state);
	reset();
	debug("state %d",state);
	return true;
}

TEST(test2) {
	reset();
	debug("state %d",state);
}
